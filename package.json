{
  "name": "webxr-native",
  "version": "0.2.25",
  "description": "WebXR native polyfill for iOS and Android via Capacitor and Desktop via Electron.",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "yarn": ">= 0.18.0"
  },
  "npmClient": "yarn",
  "scripts": {
    "check-errors": "tsc --noemit",
    "xlint": "yarn run prettier -- --check && yarn run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "yarn run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "yarn run build"
  },
  "author": "XR3 Group",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/core": "3.3.3",
    "@capacitor/ios": "next",
    "@ionic/prettier-config": "2.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "swiftlint": "1.0.1",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.6 || ^3.0.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "WebxrNative.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XRFoundation/webxr-native.git"
  },
  "bugs": {
    "url": "https://github.com/XRFoundation/webxr-native/issues"
  },
  "homepage": "https://github.com/XRFoundation/webxr-native#readme",
  "gitHead": "5ff55be1619e188226c4ac91c92a44b7ba18edf4"
}
